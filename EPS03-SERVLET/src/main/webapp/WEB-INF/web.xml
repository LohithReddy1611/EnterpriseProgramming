<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>ServletInterfaceDemo</servlet-name>
  <servlet-class>com.klef.ServletInterfaceDemo</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>GenericServletDemo</servlet-name>
  <servlet-class>com.klef.GenericServletDemo</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>HttpServletDemo</servlet-name>
  <servlet-class>com.klef.HttpServletDemo</servlet-class>
  </servlet>
   <servlet>
  <servlet-name>ServletInterfaceDemo1</servlet-name>
  <servlet-class>com.klef.ServletInterfaceDemo1</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>GenericServletDemo1</servlet-name>
  <servlet-class>com.klef.GenericServletDemo1</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>HttpServletDemo1</servlet-name>
  <servlet-class>com.klef.HttpServletDemo1</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ServletInterfaceDemo</servlet-name>
  <url-pattern>/sid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>GenericServletDemo</servlet-name>
  <url-pattern>/gsd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>HttpServletDemo</servlet-name>
  <url-pattern>/hsd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>ServletInterfaceDemo1</servlet-name>
  <url-pattern>/sid1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>GenericServletDemo1</servlet-name>
  <url-pattern>/gsd1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>HttpServletDemo1</servlet-name>
  <url-pattern>/hsd1</url-pattern>
  </servlet-mapping>
</web-app>